#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
display_board(board)
puts "Type your placement location. 1 is top left, 9 is bottom right"
input = gets.strip.to_i
input_to_index(input)
move(board, input)